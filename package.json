{
  "name": "@alexasomba/better-auth-paystack",
  "version": "0.2.1",
  "description": "Community Better Auth plugin for Paystack (by alexasomba)",
  "license": "MIT",
  "author": "alexasomba",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexasomba/better-auth-paystack.git"
  },
  "keywords": [
    "paystack",
    "auth",
    "better-auth"
  ],
  "type": "module",
  "packageManager": "pnpm@10.26.0",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./client": {
      "types": "./dist/client.d.mts",
      "default": "./dist/client.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "bd": "node ./scripts/bd.mjs",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:integration": "RUN_INTEGRATION_TESTS=1 vitest run",
    "coverage": "vitest run --coverage",
    "lint:package": "publint run --strict",
    "lint:types": "attw --profile esm-only --pack .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.27.0",
      "rollup": "npm:@rollup/wasm-node@4.53.5"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "better-auth": ">=1.3.0"
  },
  "dependencies": {
    "@alexasomba/paystack-node": "^1.1.6",
    "better-call": "^1.2.1",
    "defu": "^6.1.4",
    "zod": "^4.3.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.57.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@better-auth/core": "^1.4.18",
    "@rollup/rollup-linux-x64-gnu": "^4.57.1",
    "@types/node": "^22.19.7",
    "@vitest/coverage-v8": "^4.0.18",
    "better-auth": "^1.4.18",
    "publint": "^0.3.17",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}